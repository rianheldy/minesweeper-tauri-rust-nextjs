{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "ui:dev",
        "type": "shell",
        "isBackground": true,
        "command": "npm",
        "args": ["run","dev"],
        "problemMatcher": {
          "base": "$tsc-watch",
          "applyTo": "allDocuments"
        }
      },
      {
        "label": "ui:build",
        "type": "shell",
        "command": "npm",
        "args": ["run","build"],
        "problemMatcher": {
          "base": "$tsc-watch",
          "applyTo": "allDocuments"
        }
      }
    ]
  }